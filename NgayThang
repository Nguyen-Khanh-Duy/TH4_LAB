package thuchanh4;
import java.util.ArrayList;
import java.util.List;

public class NgayThang {
    private int ngay;
    int thang;
    private int nam;

    public NgayThang(int ngay, int thang, int nam) {
        this.ngay = ngay;
        this.thang = thang;
        this.nam = nam;
    }

    @Override
    public String toString() {
        return ngay + "/" + thang + "/" +nam;
    }
    }
